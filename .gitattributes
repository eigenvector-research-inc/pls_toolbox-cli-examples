.gitattributes export-ignore
.gitignore export-ignore

# Set the default behavior, in case people don't have core.autocrlf set.
* text=auto

# Explicitly declare files that will always have LF line endings.
*.sh    text eol=lf
*.py    text eol=lf
*.json  text eol=lf

# Explicitly declare files that will always have CRLF line endings.
*.bat   text eol=crlf
*.cmd   text eol=crlf

# Files to be tracked by git-lfs (e.g. binary files)
*.mat filter=lfs diff=lfs merge=lfs -text
PLS_Manual.pdf filter=lfs diff=lfs merge=lfs -text
PLS_Toolbox_Quick_Reference.pdf filter=lfs diff=lfs merge=lfs -text
*.PDF filter=lfs diff=lfs merge=lfs -text
*.svg filter=lfs diff=lfs merge=lfs -text
*.ttf filter=lfs diff=lfs merge=lfs -text
